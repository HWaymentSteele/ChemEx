rm -fr output

echo "Global Test"
chemex_fit.py -e experiments/cest_n15*.cfg -p parameters/params_n15.cfg -o output/global

echo "Exclusion Test"
chemex_fit.py -e experiments/cest_n15*.cfg -p parameters/params_n15.cfg -r S41N-HN -o output/no_41

echo "Inclusion Test"
chemex_fit.py -e experiments/cest_n15*.cfg -p parameters/params_n15.cfg +r S41N-HN -o output/just_41

echo "Method Test"
chemex_fit.py -e experiments/cest_n15*.cfg -p parameters/params_n15.cfg -m methods/method_n15.cfg -o output/method
